generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id           String   @id @default(cuid())
  clerkUserId  String   @unique  
  email        String
  credits      Int      @default(3) 
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
